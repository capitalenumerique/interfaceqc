{
    "format": "custom",
    "id": "volunteers",
    "label": "Volunteers",
    "repeatable": true,
    "status": true,
    "json": {
        "Main": {
            "uid": {
                "type": "UID",
                "config": {
                    "label": "UID"
                }
            },
            "name": {
                "type": "Text",
                "config": {
                    "label": "name",
                    "placeholder": ""
                }
            },
            "img": {
                "type": "Image",
                "config": {
                    "label": "img",
                    "constraint": {
                        "width": 290,
                        "height": 290
                    },
                    "thumbnails": []
                }
            },
            "linkedin": {
                "type": "Link",
                "config": {
                    "label": "linkedin",
                    "placeholder": "",
                    "select": null,
                    "allowTargetBlank": false,
                    "allowText": false,
                    "repeat": false
                }
            },
            "comitee": {
                "type": "Select",
                "config": {
                    "label": "Comitee",
                    "placeholder": "",
                    "options": [
                        "Comité programmation",
                        "Comité design",
                        "Comité web",
                        "Comité communication",
                        "Comité logistique",
                        "Comité délégations",
                        "Comité XP conférenciers",
                        "Comité technique"
                    ]
                }
            },
            "committee_head": {
                "type": "Boolean",
                "config": {
                    "label": "Committee head",
                    "placeholder_false": "false",
                    "placeholder_true": "true",
                    "default_value": false
                }
            }
        }
    }
}
