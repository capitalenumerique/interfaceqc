{
  "name": "interfaceqc",
  "private": true,
  "type": "module",
  "scripts": {
    "get-git-env": "NUXT_ENV_CURRENT_GIT_SHA=`git rev-parse --short HEAD` NUXT_ENV_CURRENT_GIT_TAG=`git describe --tags $(git rev-list --tags --max-count=1)`",
    "build": "pnpm run get-git-env nuxt build",
    "dev": "pnpm run get-git-env nuxt dev",
    "generate": "pnpm run get-git-env nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "slicemachine": "start-slicemachine"
  },
  "dependencies": {
    "nuxt": "^3.14.0",
    "vue": "latest"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.6.1",
    "@nuxt/image": "^1.8.1",
    "@nuxtjs/i18n": "^9.0.0",
    "@nuxtjs/prismic": "^3.4.3",
    "@slicemachine/adapter-nuxt": "^0.3.55",
    "@types/eslint-config-prettier": "^6.11.3",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "slice-machine-ui": "^2.10.1",
    "typescript": "^5.6.3",
    "vite-plugin-eslint2": "^5.0.1",
    "vite-svg-loader": "^5.1.0"
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228"
}
