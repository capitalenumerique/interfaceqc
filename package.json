{
  "name": "interfaceqc",
  "private": true,
  "type": "module",
  "scripts": {
    "get-git-env": "NUXT_ENV_CURRENT_GIT_SHA=`git rev-parse --short HEAD` NUXT_ENV_CURRENT_GIT_TAG=`git describe --tags $(git rev-list --tags --max-count=1)`",
    "build": "pnpm run get-git-env nuxt build",
    "dev": "concurrently \"pnpm:nuxt:dev\" \"pnpm:slicemachine\" --names \"nuxt,slicemachine\" --prefix-colors green,magenta",
    "nuxt:dev": "pnpm run get-git-env nuxt dev",
    "generate": "pnpm run get-git-env nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "slicemachine": "start-slicemachine"
  },
  "dependencies": {
    "focus-trap": "^7.6.4",
    "normalize.css": "^8.0.1",
    "nuxt": "^3.16.0",
    "swiper": "^11.2.5",
    "vue": "^3.5.13",
    "vue3-marquee": "^4.2.2"
  },
  "devDependencies": {
    "@csstools/postcss-global-data": "^3.0.0",
    "@nuxt/eslint": "^1.2.0",
    "@nuxt/fonts": "^0.11.0",
    "@nuxt/image": "^1.9.0",
    "@nuxtjs/i18n": "^9.3.1",
    "@nuxtjs/prismic": "^4.1.0",
    "@slicemachine/adapter-nuxt": "^0.3.69",
    "@types/eslint-config-prettier": "^6.11.3",
    "@vueuse/components": "^13.0.0",
    "@vueuse/core": "^13.0.0",
    "@vueuse/integrations": "^13.0.0",
    "@vueuse/motion": "^3.0.3",
    "@vueuse/nuxt": "^13.0.0",
    "concurrently": "^9.1.2",
    "eslint": "9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "postcss": "^8.5.3",
    "postcss-functions": "^4.0.2",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.1.5",
    "slice-machine-ui": "^2.13.2",
    "typescript": "^5.8.2",
    "vite-plugin-eslint2": "^5.0.3",
    "vite-svg-loader": "^5.1.0"
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228"
}
