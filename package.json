{
  "name": "interfaceqc",
  "private": true,
  "type": "module",
  "scripts": {
    "get-git-env": "NUXT_ENV_CURRENT_GIT_SHA=`git rev-parse --short HEAD` NUXT_ENV_CURRENT_GIT_TAG=`git describe --tags $(git rev-list --tags --max-count=1)`",
    "build": "pnpm run get-git-env nuxt build",
    "dev": "concurrently \"pnpm:nuxt:dev\" \"pnpm:slicemachine\" --names \"nuxt,slicemachine\" --prefix-colors green,magenta",
    "nuxt:dev": "pnpm run get-git-env nuxt dev",
    "generate": "pnpm run get-git-env nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "slicemachine": "start-slicemachine"
  },
  "dependencies": {
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/schema": "^4.1.3",
    "@nuxt/scripts": "^0.11.13",
    "@peterbud/nuxt-query": "^1.2.0",
    "@prismicio/client": "^7.20.3",
    "@tanstack/vue-query": "^5.90.5",
    "@unhead/vue": "^2.0.19",
    "es-toolkit": "^1.40.0",
    "focus-trap": "^7.6.5",
    "graphql": "^16.11.0",
    "graphql-request": "^7.3.1",
    "luxon": "^3.7.2",
    "normalize.css": "^8.0.1",
    "nuxt": "^4.1.3",
    "swiper": "^11.2.10",
    "vue": "^3.5.22",
    "vue3-marquee": "^4.2.2"
  },
  "devDependencies": {
    "@csstools/postcss-global-data": "^3.1.0",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/image": "^1.11.0",
    "@nuxtjs/i18n": "^10.1.1",
    "@nuxtjs/prismic": "^4.1.0",
    "@slicemachine/adapter-nuxt": "^0.3.84",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/luxon": "^3.7.1",
    "@vueuse/components": "^13.9.0",
    "@vueuse/core": "^13.9.0",
    "@vueuse/integrations": "^13.9.0",
    "@vueuse/motion": "^3.0.3",
    "@vueuse/nuxt": "^13.9.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "postcss": "^8.5.6",
    "postcss-functions": "^4.0.2",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.4.0",
    "slice-machine-ui": "^2.18.2",
    "typescript": "^5.9.3",
    "vite-plugin-eslint2": "^5.0.4",
    "vite-svg-loader": "^5.1.0"
  },
  "packageManager": "pnpm@10.19.0"
}
